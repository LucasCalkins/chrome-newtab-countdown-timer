{
  "name": "Countdown",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Powerful, customizable countdown timers to boost productivity every time you open a new tab.",
  "homepage_url": "https://github.com/raviriley",
  "author": Ravi Riley,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  },
  "permissions": ["storage","management","notifications"],
  "offline_enabled": true
}