{
    "name": "Countdown",
    "description": "Powerful, customizable countdown timers to boost productivity every time you open a new tab.",
    "version": "0.0.1",
    "manifest_version": 2,
    "icons": {
        "48": "extension-icons/test.png",
        "128": "extension-icons/test.png"
    },
    "chrome_url_overrides": {
        "newtab": "countdown.html"
    },
    "permissions": ["storage"],
	"offline_enabled": true,
	"options_page": "options.html",
}
